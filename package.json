{
  "name": "@epilot/epilot-journey-sdk",
  "version": "1.0.6",
  "author": "Mohannad Mamo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "release": "standard-version && npm publish",
    "prepublishOnly": "yarn build",
    "postrelease": "git push && git push --tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@material-ui/core": "4.*",
    "standard-version": "^9.5.0",
    "tsup": "^7.0.0"
  },
  "peerDependencies": {
    "@material-ui/core": "4.*"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": false,
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "external": []
  }
}
